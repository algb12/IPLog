<div class="report">
    <h2>{{title}}</h2>
    <table class="reportTbl">
        <tr>
            <td>Trigger query:</td>
            <td>{{query}}</td>
        </tr>
        <tr>
            <td>Logged:</td>
            <td>{{logtime}}</td>
        </tr>
        <tr>
            <td>Client IP:</td>
            <td>{{clientIP}}</td>
        </tr>
        <tr>
            <td>Country:</td>
            <td>{{country}}</td>
        </tr>
        <tr>
            <td>City:</td>
            <td>{{city}}</td>
        </tr>
        <tr>
            <td>ISP:</td>
            <td>{{isp}}</td>
        </tr>
    </table>
</div>
